<div id= "star_<%= report.id %>"></div>
<script>
  $(document).on('turbolinks:load', function() {
    let elem = document.querySelector('#star_<%= report.id %>');
    if (elem == null) return;

    elem.innerHTML = "";
    let opt = {  
      starOn: "<%= asset_path('star-on.png') %>",
      starOff: "<%= asset_path('star-off.png') %>",
      score: "<%= report.star %>",
      readOnly: true,
    };
    raty(elem, opt);
  });
</script>